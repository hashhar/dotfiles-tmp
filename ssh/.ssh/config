# The clone URL for this setup will be git@[Host]:[github username]/[repo]
# ie. git@github.com:hashhar/dotfiles and git@work.github.com:hashhar/workdotfiles
PreferredAuthentications	publickey,keyboard-interactive,password
AddKeysToAgent			yes
IdentitiesOnly			yes

Host github.com
	HostName		github.com
	User			git
	IdentityFile		~/.ssh/id_rsa_delhivery

Host hashhar.github.com
	HostName		github.com
	User			git
	IdentityFile		~/.ssh/id_rsa

Host bitbucket.org
	HostName		bitbucket.org
	User			git
	IdentityFile		~/.ssh/id_rsa_delhivery

Host *
	ServerAliveInterval	240
	ServerAliveCountMax	2

Host *.amazonaws.com
	IdentityFile		~/aws/ashharhasandelhiverycom.pem
	StrictHostKeyChecking	no
	UserKnownHostsFile	/dev/null

Host dev-godam-bastion
	HostName		13.228.62.16
	User			ubuntu
	IdentityFile		~/.ssh/id_rsa_delhivery
	StrictHostKeyChecking	no

Host prod-godam-bastion
	HostName		18.136.225.226
	User			ubuntu
	IdentityFile		~/.ssh/id_rsa_delhivery
	StrictHostKeyChecking	no

# Elasticsearch Tunnel
Host estunnel
	HostName		18.136.225.226
	User			ubuntu
	IdentityFile		~/.ssh/id_rsa_delhivery
	StrictHostKeyChecking	no
	LocalForward 		19200 vpc-wms2-e2e-vcfytsmwcr7vtegxjvvcmq67ui.ap-southeast-1.es.amazonaws.com:443

Host prod-dwh-bastion
