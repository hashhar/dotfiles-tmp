# The clone URL for this setup will be git@[Host]:[github username]/[repo]
# ie. git@github.com:hashhar/dotfiles and git@work.github.com:hashhar/workdotfiles
PreferredAuthentications	publickey,password
AddKeysToAgent				yes
IdentitiesOnly				yes

Host hashhar.github.com
	HostName			github.com
	User				git
	IdentityFile		~/.ssh/id_rsa
