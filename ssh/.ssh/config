# The clone URL for this setup will be git@[Host]:[github username]/[repo]
# ie. git@github.com:hashhar/dotfiles and git@work.github.com:hashhar/workdotfiles
PreferredAuthentications	publickey,password
AddKeysToAgent				yes
IdentitiesOnly				yes

Host jabong.github.com
	HostName			github.com
	User				git
	IdentityFile		~/.ssh/id_rsa_jabong

Host hashhar.github.com
	HostName			github.com
	User				git
	IdentityFile		~/.ssh/id_rsa

Host bitbucket.org
	HostName			bitbucket.org
	User				git
	IdentityFile		~/.ssh/id_rsa_jabong

Host amardeep
	HostName			amardeep
	User				Amardeep
	IdentityFile		~/.ssh/id_rsa_jabong

Host rishabh
	HostName			rishabh
	User				Rishabh
	IdentityFile		~/.ssh/id_rsa_jabong

Host db18
	HostName			db18
	User				amardeep
	IdentityFile		~/.ssh/id_rsa_jabong

Host newapis*
	User				amardeep
	IdentityFile		~/.ssh/master

Host logstash
	HostName			logstash
	User				ashhar
	IdentityFile		~/.ssh/id_rsa_jabong

Host sc-qa-fe
	HostName			sc-qa-fe
	User				ashar
	IdentityFile		~/.ssh/id_rsa_jabong

